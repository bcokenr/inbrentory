datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Item {
  id          Int      @id @default(autoincrement())
  name        String
  description String?
  cost_basis  Int?
  transaction_price  Int?
  transaction_id  Int?
  list_price Int
  discounted_list_price Int?
  has_printed_tag Boolean @default(false)
  store_credit_amount_applied Int?
  keywords   String?
  categories String?
  measurements String?
  quantity    Int       @default(1)
  createdAt   DateTime  @default(now())
  updatedAt   DateTime  @updatedAt
}
